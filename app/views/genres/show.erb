<h1>Songs in <%= @genre.name %></h1>
<br>
<ul>
    <% @genre.songs.each do |song| %>
      <li>
      <a href="/songs/<%= song.slug %>"><%= song.name %></a>
      </li>
    <% end %>
</ul>
<br>
<h1>Artists in <%= @genre.name %></h1>
<br>
<ul>
    <% @genre.arstists.each do |artist| %>
        <li>
        <a href="/artists/<%= artist.slug %>"><%= artist.name %></a>
        </li>
    <% end %>   
</ul>
<br>
<a href="/">Home</a>